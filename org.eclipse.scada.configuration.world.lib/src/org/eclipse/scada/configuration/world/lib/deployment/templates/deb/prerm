#!/bin/sh
#
# see: dh_installdeb(1)

set -e

. /usr/share/debconf/confmodule

case "$1" in
  remove|deconfigure)
$$prerm.stop$$
  ;;

  upgrade|failed-upgrade)
  ;;

  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0