<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.neoscada.deploy.console.feature"
      label="%featureName"
      version="0.5.0.qualifier"
      provider-name="%providerName"
      license-feature="org.eclipse.license"
      license-feature-version="1.0.1.qualifier">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.equinox.console" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.console.ssh" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.apache.felix.gogo.command" version="0.10.0" match="greaterOrEqual"/>
      <import plugin="org.apache.felix.gogo.runtime" version="0.10.0" match="greaterOrEqual"/>
      <import plugin="org.apache.felix.gogo.shell" version="0.10.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi" version="3.10.0" match="greaterOrEqual"/>
      <import plugin="org.apache.sshd.core" version="0.7.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.license" version="1.0.1"/>
   </requires>

   <plugin
         id="org.eclipse.equinox.console.jaas.fragment"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

</feature>
